#include <stdio.h>
#include <time.h>
void data(void){

   int dia,mes,ano;
   struct tm *local;
   time_t t;

   t = time(NULL);
   local = localtime(&t);

   dia = local -> tm_mday;
   mes = local -> tm_mon + 1;
   ano = local -> tm_year + 1900;

   printf("%i/",dia);
   printf("%i/",mes);
   printf("%i",ano);

}
int main()
{
   data();
}
